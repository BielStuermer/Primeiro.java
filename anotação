// Importa a classe Scanner para ler dados do teclado
import java.util.Scanner;

// Classe principal do programa
public class TotalEleitores {
	public static void main(String[] args) {
		// Cria um objeto Scanner para ler dados do teclado
		Scanner sc = new Scanner(System.in);

		// Solicita e lê o número total de eleitores
		System.out.print("Digite o número total de eleitores: ");
		int total = sc.nextInt();

		// Solicita e lê o número de votos brancos
		System.out.print("Digite o número de votos brancos: ");
		int brancos = sc.nextInt();

		// Solicita e lê o número de votos nulos
		System.out.print("Digite o número de votos nulos: ");
		int nulos = sc.nextInt();

		// Solicita e lê o número de votos válidos
		System.out.print("Digite o número de votos válidos: ");
		int validos = sc.nextInt();

		// Calcula o percentual de votos brancos em relação ao total
		double percBrancos = (brancos * 100.0) / total;
		// Calcula o percentual de votos nulos em relação ao total
		double percNulos = (nulos * 100.0) / total;
		// Calcula o percentual de votos válidos em relação ao total
		double percValidos = (validos * 100.0) / total;

		// Exibe os percentuais calculados
		System.out.printf("Percentual de votos brancos: %.2f%%\n", percBrancos);
		System.out.printf("Percentual de votos nulos: %.2f%%\n", percNulos);
		System.out.printf("Percentual de votos válidos: %.2f%%\n", percValidos);

		// Fecha o Scanner para liberar o recurso
		sc.close();
	}
}